students = ["Thomas", "Simon", "Joseph", "Moses"]

for _ in students:
    print(_)

#The above can also be written as
students = ["Thomas", "Simon", "Joseph", "Moses"]

for student in students:
    print(student)

#Using the length variable
students = ["Thomas", "Simon", "Joseph", "Moses"]

for k in range(len(students)):
    print(students[k])